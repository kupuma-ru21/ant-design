{
  "name": "ant-design",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "//": "REF: https://the-guild.dev/graphql/codegen/docs/config-reference/require-field > Customize loaded env file",
    "codegen": "DOTENV_CONFIG_PATH='../.env' graphql-codegen --require dotenv/config --config codegen.ts",
    "dev": "pnpm codegen && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:tsc": "tsc --incremental false --noEmit"
  },
  "dependencies": {
    "@ant-design/nextjs-registry": "^1.0.2",
    "@apollo/client": "^3.12.7",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@next/env": "^15.1.6",
    "antd": "^5.23.2",
    "graphql": "^16.10.0",
    "next": "15.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@eslint/eslintrc": "^3",
    "@graphql-codegen/cli": "^5.0.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "typescript": "^5.7.3"
  }
